<div class="admin-content-header">
  <div class="admin-content-header-title">
  <h2>
  Project Types
</h2>
</div>
<div class="admin-content-header-action">
  <%= link_to 'New Project Type',  new_admin_project_type_path, class: "btn btn-outline-info" %>
</div>
</div>



<ul class="items-listing">
  <li class="item-listing-inner table-title">
      <div class="listing-items date"><b>Date</b></div>
      <div class="listing-items title"><b>Title</b></div>
      <div class="listing-items page-assignment"><b></b></div>
      <div class="listing-items action">
        <b>Action</b>
      </div>
    </li>
  <% @ProjectTypes.each do |pt| %>
    <li class="item-listing-inner">
      <div class="listing-items date"><%= pt.created_at.strftime("%b %d, %Y") %></div>
      <div class="listing-items title"><b><%= pt.name %></b></div>
      <div class="listing-items page-assignment"> </div>
      <div class="listing-items action">
        <%= link_to '', edit_admin_project_type_path(pt), class: "fa fa-edit" %>
        <%= link_to '', admin_project_type_url(pt), method: :delete, data: { confirm: 'Are you sure?' }, class: "fa fa-trash" %>
      </div>
    </li>
  <% end %>
  
</ul>

